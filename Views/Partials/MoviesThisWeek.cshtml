<div class="container">
    <div class="row" style="margin-left:50px;">
        @foreach (ScheduleModel schedule in Model)
        {
            <div class="col-md-4">
                @await Html.PartialAsync("~/Views/Partials/MovieCard.cshtml", MovieModel.GetModelByID(schedule.MovieID))
            </div>
        }
    </div>
</div>
